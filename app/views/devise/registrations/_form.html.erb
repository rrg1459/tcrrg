<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <div class="form-group">
    <%= f.text_field :name, class: "form-control", placeholder: "Nombre Completo" %>
  </div>
  <div class="form-group">
    <%= f.text_field :email, class: "form-control", placeholder: "Correo" %>
  </div>
  <div class="form-group">
    <%= f.text_field :username, class: "form-control", placeholder: "Usuario" %>
  </div>
  <div class="form-group">
    <%= f.password_field :password, class: "form-control", placeholder: "ContraseÃ±a" %>
  </div>
  <div class="form-group">
    <%= f.submit 'Crear', class: "btn btn-warning" %>
  </div>
  <%= render 'partials/devise_form_errors' %>
<% end %>
